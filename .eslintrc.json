{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:storybook/recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn",
    "no-empty-function": "warn",
    "prettier/prettier": "warn"
  }
}
